crontab -l >/dev/null 2>&1 || exit 0
crontab -l 2>/dev/null | grep -q simetbox_register_ma.sh && exit 0

( crontab -l ;
  echo simetbox_register_ma.sh | awk '{ srand(); print int(60*rand()) " " int(24*rand()) " * * * " $0 }' ) | crontab -
:
